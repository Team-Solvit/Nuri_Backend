type UserInfo {
    id : String
    userId : String
    country : String
    language : String
    name : String
    email : String
    introduce : String
    profile : String
    role : Role
}

enum Role {
    ANONYMOUS
    USER
    INTERNATIONAL_STUDENT
    HOST
    THIRD_PARTY
    OPERATOR
}

type HostInfo {
    callNumber : String
    userInfo : UserInfo
}

type BoarderInfo {
    callNumber : String
    gender : String
    userInfo : UserInfo
}